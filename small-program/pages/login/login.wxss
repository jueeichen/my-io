.container {
  width: 100vw;
  height: 100vh;
  text-align: center;
}

image {
  width: 154rpx;
  height: 227rpx;
  display: inline-block;
  margin-top: 95rpx;
}

.login-box {
  margin-top: 70rpx;
  width: 100vw;
  height: auto;
  text-align: center;
}

.login-box button {
  display: inline-block;
  width: 690rpx;
  height: 98rpx;
  border: none;
  color: white;
  background: rgba(3, 192, 95, 1);
  border-radius: 10rpx;
}

.font_bold {
  padding: 0 25rpx;
  margin-top: 110rpx;
  text-align: left;
  color: #333;
  font-size: 30rpx;
  font-weight: bold;
}

.font_normal {
  padding: 0 30rpx;
  margin-top: 28rpx;
  text-align: left;
  color: #666;
  font-size: 28rpx;
  font-weight: normal;
}

.botom_font{
position: fixed;
width: 100vw;
height: auto;
left: 0;
bottom:20rpx;
}
.botom_font_normal{
  display: inline-block;
  color:#999999;

}
.botom_font_bold{
  display: inline-block;
  color:#333333;
  
}
