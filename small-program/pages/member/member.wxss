.memberContainer {
  background-color: white;
}

.memberHeadTop {
  height: 416rpx;
  background-color: #fff;
  position: relative;
}

.memberHead {
  width: 100%;
  height: 416rpx;
}

.memberHead_bg {
  width: 100vw;
  height: 300rpx;
}

.memTop {
  margin: 0 46rpx 0;
  display: flex;
  justify-content: space-between;
  position: absolute;
  top: 0;
  left: 0;
  width: 670rpx;
}

.information {
  display: flex;
}

.information .avatar {
  width: 110rpx;
  height: 110rpx;
}

.information .infText {
  margin-left: 36rpx;
  color: #fff;
}

.infText .userName {
  margin: 20rpx 0 12rpx 0;
  font-size: 28rpx;
}

.infText .userId {
  font-size: 24rpx;
}

.avatar image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.mesageBox {
  width: 67rpx;
  height: 67rpx;
  margin: 10rpx 10rpx 0 0;
  position: relative;
}

.mesageBox .mesLink {
  width: 100%;
  height: 100%;
  display: block;
}

.mesageBox .mesIco {
  width: 49rpx;
  height: 49rpx;
  margin: 17rpx 0 0 17rpx;
}

.mesageBox .dot {
  position: absolute;
  top: 12rpx;
  right: 2rpx;
  width: 18rpx;
  height: 18rpx;
  background-color: #f54937;
  border-radius: 50%;
}

.moneyDepositWrap {
  position: relative;
  display: flex;
  justify-content: space-between;
  margin-top: -150rpx;
  width: 100vw;
  height: 198rpx;
}

.moneyDepositWrap_bg {
  position: absolute;
  top: 0;
  width: 748rpx;
  height: 202rpx;
  display: inline-block;
}

.moneyText {
  position: relative;
  display: flex;
  align-items: center;
  flex: 1;
  margin-top: 18rpx;
  height: 142rpx;
  margin-left: 64rpx;
  font-size: 24rpx;
  color: #999;
}

.moneyText .money {
  font-size: 28rpx;
  color: #28bdf0;
  margin-left: 30rpx;
}

.depositText {
  position: relative;
  display: flex;
  align-items: center;
  flex: 1;
  margin-top: 18rpx;
  height: 142rpx;
  font-size: 24rpx;
  color: #999;
  margin-left: 80rpx;
}

.depositText .deposit {
  font-size: 28rpx;
  color: #28bdf0;
  margin-left: 30rpx;
}

.moneyDivid {
  width: 14rpx;
  height: 27rpx;
  margin-top: 70rpx;
  margin-left: 25rpx;
  position: relative;
}

.moneyDivid image {
  width: 100%;
  height: 100%;
}

.moneyDepositWrap .returnDepositMoney {
  width: 263rpx;
  height: 93rpx;
  position: absolute;
  bottom: -15rpx;
  left: 50%;
  margin-left: -131rpx;
}

.returnDepositMoney .returnDepositMoneyBtn {
  width: 100%;
  height: 100%;
}

.returnDepositMoneyImg {
  width: 100%;
  height: 100%;
}

