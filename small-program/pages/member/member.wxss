.memlist_li {
  box-sizing: border-box;
  float: left;
  height: 94rpx;
  width: 100vw;
  padding: 0rpx 0 0rpx 40rpx;
  font-size: 30rpx;
  color: #333;
}

.memlist_li_main {
  float: left;
  width: 40rpx;
  height: 40rpx;
}

.memlist_li_content {
  box-sizing: border-box;
  float: right;
  width: 639rpx;
  padding-right: 40rpx;
  position: relative;
}

.memlist_li_right {
  float: right;
  width: 13rpx;
  height: 24rpx;
  margin-top: 10rpx;
}

.memlist_li_content::after {
  display: block;
  content: "";
  position: absolute;
  width: 100%;
  height: 1rpx;
  background-color: #eee;
  top: 65rpx;
}

/* 弹窗 */

.pop_windows {
    width: 100vw;
  height: 100vh;
  text-align: center;
  position: fixed;
  left: 0;
  top:0;
}

.pop_mark {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 99
}

.pop_content {
  width: 542rpx;
  height: 364rpx;
  background-color: white;
  position:relative;
  z-index: 100;
  display: inline-block;
  top:35%;
  border-radius: 10rpx;
}

.pop_content image {
  width: 232rpx;
  height: 232rpx;
  display: inline-block;
  margin-top: -100rpx;
}

.pop_content text {
  display: block;
  color:#666666;
  height: 100rpx;
}

.pop_btn{
margin-top: 30rpx;

  display: block;
  box-sizing: border-box;
  width: 100%;
  height: 80rpx;
  padding-top:20rpx; 
  color:#FF7123;
  border-top:1rpx solid #eee;
  font-size: 34rpx;
}
.pop_btn_right{
  box-sizing: border-box;
  display: inline-block;
  width:50%;
  height: 100%;
  border-right: 1rpx solid #eee
}
.pop_btn_no{
  display: inline-block;
   width:50%;
  height: 100%;

}

.memlist_padding{
  margin-top:-10rpx;
}

.memlist_ul{
  padding-top:12rpx;

}