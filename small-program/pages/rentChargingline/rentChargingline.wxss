.rentLineContainer {
  position: fixed;
  width: 100vw;
  height: 100vh;
  background-color: #f7f7f7
}

.rentChargingline_bg {
  position: absolute;
  z-index: 0;
  height: 310rpx;
  width: 100vw;
}

.rentLineWrap {
  position: relative;
  z-index: 1;
  width: 100%;
  height: 100%;
}

.rentLineHead{
  width:100%;
  height: 381rpx;
  color:#fff;
  position: relative;
}
.lineHTop{
  display: flex;
  height: 58rpx;
  padding-top: 18rpx;
  align-items: center;
  justify-content: space-between
}
.lineHTop .sn{
  font-size: 30rpx;
  margin-left: 38rpx;
}
.lineHTop .records{
  display: inline-block;
  width:182rpx;
  height: 58rpx;
  line-height: 58rpx;
  background-color: rgba(255,255,255,0.7);
  color: #FF7F0B;
  border-top-left-radius: 40rpx;
  border-bottom-left-radius: 40rpx;
  font-size: 26rpx;
  /* text-indent: 22rpx; */
  text-align: center;
  position: relative;
}
.records image{
  width:12rpx;
  height: 24rpx;
  /* margin: 0.5rpx 0 0 1rpx; */
 transform: translateY(5rpx)

}
.lineHDes{
  margin: 18rpx 52rpx 0 52rpx;
  line-height: 40rpx;
  font-size: 28rpx;
}


.rentlineAgreement{
  font-size: 26rpx;
  margin: 30rpx 0 0 0;
  color: #666666;
  position: absolute;
  bottom: 10px;
  text-align: center;
  width: 100vw;
}
.rentlineAgreement .agreementLink{
  display: inline-block;
  color: #FF7F0B;
}


.rentlineBoxLists {
  box-sizing: border-box;
  flex-direction:column;
  background-color: #fff;
}

.rentListBox {
  display: block;
  top: -80px;
  width: 94%;
  height: 105px;
  line-height: 105px;
  margin: 10px auto;
  box-sizing: border-box;
  position: relative;
  background-color: transparent;
  border: none!important;
}




.rentListBox image {
  width: 100%;
  height: 100%;
  z-index: -1;
  position: absolute;
  top: 0;
  left: 0;
}
.rentListBox_li{
background-color: transparent;
}
.rentListBox .boxLink {
  border: none;
  width: 100%;
  height: 100%;
  background-color: transparent;
  position: absolute;
  z-index: 9;
  left: 0;
  top:0;
}
.boxLink::after{
  border: none!important;
}
.rentListBox .chargeTime {
  font-size: 14px;
  color: #666666;
  text-indent: 15px;
  z-index: 1;
  position: absolute;
  left: 0;
  top: 0;
}

.rentListBox .chargeMoney {
  position: absolute;
  right: 30px;
  top: 50%;
  transform: translateY(-50%);
  color: #FF7F0B;
}

.rentListBox .chargeMoney .money {
  font-size: 24px;
  font-weight: 700;
}
